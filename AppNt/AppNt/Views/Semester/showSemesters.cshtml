@model IEnumerable<AppNt.ViewModels.SemesterViewModel>

@{
    ViewData["Title"] = "showSemesters";
}

<h1>showSemesters</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                <a asp-controller="Asignature" asp-action="showAsignatures" asp-route-id="@item.Id">Acceder a semestre</a> |
            </td>
        </tr>
}
    </tbody>
</table>
