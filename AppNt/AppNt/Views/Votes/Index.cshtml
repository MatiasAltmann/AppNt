@model IEnumerable<AppNt.Models.Vote>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead   class="thead-dark">
        <tr>
            <th cope="col">
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th cope="col">
                @Html.DisplayNameFor(model => model.Profesor)
            </th>
            <th cope="col">
                @Html.DisplayNameFor(model => model.valueVote)
            </th>
            <th cope="col">More information</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.User.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Profesor.Lastname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.valueVote)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
            </td>
        </tr>
}
    </tbody>
</table>
